{
  "name": "stetholink-ai",
  "version": "1.0.0",
  "description": "Cross-platform multilingual medical diagnostic chatbot for WhatsApp and Telegram",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server-railway-advanced.js",
    "start-standalone": "node start-standalone.js",
    "dev": "nodemon src/server.js",
    "railway:start": "node src/server-railway.js",
    "test": "node test-advanced-features.js",
    "test:production": "node test-production-readiness.js",
    "deploy:heroku": "node deploy-heroku-backend.js",
    "deploy:railway": "node deploy-railway-backend.js",
    "test-mobile": "node test-mobile-app.js",
    "build": "node build.js",
    "build:netlify": "npm run build && echo 'Netlify Functions ready'",
    "build:railway": "node build-railway.js",
    "build:mobile": "node build-mobile-app.js",
    "build:android": "npm run build:mobile && npx cap build android",
    "build:ios": "npm run build:mobile && npx cap build ios",
    "add:android": "npx cap add android",
    "add:ios": "npx cap add ios",
    "sync": "npx cap sync",
    "open:android": "npx cap open android",
    "open:ios": "npx cap open ios",
    "run:android": "npx cap run android",
    "run:ios": "npx cap run ios",
    "heroku:deploy": "node deploy-heroku-now.js"
  },
  "keywords": [
    "medical",
    "chatbot",
    "whatsapp",
    "telegram",
    "ai",
    "diagnosis",
    "sri-lanka",
    "multilingual",
    "mobile-app",
    "pwa",
    "capacitor"
  ],
  "author": "StethoLink AI Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.58.0",
    "@capacitor/android": "^5.7.8",
    "@capacitor/app": "^5.0.8",
    "@capacitor/camera": "^5.0.10",
    "@capacitor/core": "^5.7.8",
    "@capacitor/device": "^5.0.8",
    "@capacitor/geolocation": "^5.0.8",
    "@capacitor/haptics": "^5.0.8",
    "@capacitor/ios": "^5.7.8",
    "@capacitor/keyboard": "^5.0.9",
    "@capacitor/local-notifications": "^5.0.8",
    "@capacitor/push-notifications": "^5.1.2",
    "@capacitor/status-bar": "^5.0.8",
    "@huggingface/inference": "^4.6.1",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^11.11.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.64.0",
    "openai": "^4.20.1",
    "pdfkit": "^0.14.0",
    "socket.io": "^4.7.4",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.7.8",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
